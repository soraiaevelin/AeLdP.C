#include <stdio.h>

int main() {
   
    int a=20;
    int b=2;
    int soma,dif,multiplicacao,divisao,resto;
    
    
    soma=a+b;
    dif=a-b;
    multiplicacao=a*b;
    divisao=a/b;
    resto=a%b;

     printf("Digite o primeiro número");
     scanf(%d, &a);

     printf("Digite o segundo número");
     scanf(%d, &b);
    
    
      printf("A soma do número:%d com o número:%d é: %d\n",a,b,soma);
      printf("A dif do número:%d com o número:%d é: %d\n",a,b,dif);
      printf("A mutiplicacao do número:%d com o número:%d é: %d\n",a,b,multiplicacao);
      printf("A divisão do número:%d com o número:%d é: %d\n",a,b,divisao);
      printf("A resto do número:%d com o número:%d é: %d\n",a,b,resto);
     
    return 0;
