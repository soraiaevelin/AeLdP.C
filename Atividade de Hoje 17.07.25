#include <stdio.h>

int main() {
 
    int numero;

    
    printf("Digite um número inteiro: ");
    scanf("%d", &numero);

    
    if (numero % 2 == 0) {
        printf("O número %d é PAR (múltiplo de 2).\n", numero);
    } else {
        printf("O número %d é ÍMPAR (não é múltiplo de 2).\n", numero);
    }

    return 0;
}


