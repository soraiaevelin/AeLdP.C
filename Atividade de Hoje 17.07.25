#include <stdio.h>

int main() {
  
    int a, b;
    
    printf("Digite o primeiro número inteiro: ");
    scanf("%d", &a);

    printf("Digite o segundo número inteiro: ");
    scanf("%d", &b);

  
    if (a % 2 == 0) {
        printf("O número %d é PAR.\n", a);
    } else {
        printf("O número %d é ÍMPAR.\n", a);
    }

  
    if (b % 2 == 0) {
        printf("O número %d é PAR.\n", b);
    } else {
        printf("O número %d é ÍMPAR.\n", b);
    }

    return 0;
}

